msgctxt "model:ir.message,text:no_numbers"
msgid "No numbers found in string \"%(number)s\"."
msgstr "No s'han trobat números a la cadena \"%(number)s\"."

msgctxt "model:ir.message,text:serial_number"
msgid ""
"Move \"%(move)s\" can not be done as its product \"%(product)s\" is marked "
"as serial number and its quantity is different than 1."
msgstr ""
"No es pot moure \"%(move)s\" perquè el seu producte \"%(product)s\" està "
"marcat com a número de sèrie i la seva quantitat és diferent de l'1."
